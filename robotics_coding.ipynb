        "# answer:\n",
        "def char_count(s):\n",
        "    count = {}\n",
        "    for c in s:\n",
        "        count[c] = count.get(c, 0) + 1\n",
        "    return dict(sorted(count.items()))\n",
        "\n",
        "print(char_count(\" Anthony\"))"
