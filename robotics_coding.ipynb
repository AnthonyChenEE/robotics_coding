      "cell_type": "code",
      "source": [
        "# answer:\n",
        "def max_subarray(nums):\n",
        "   max_sum = curr = nums[0]\n",
        "   for n in nums[1:]:\n",
        "       curr = max(n, curr + n)\n",
        "       max_sum = max(max_sum, curr)\n",
        "   return max_sum\n",
        "\n",
        "print(max_subarray([-2,1,-3,4,-1,2,1,-5,4]))\n"
      ],
