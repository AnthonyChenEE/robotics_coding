      "cell_type": "code",
      "source": [
        "# answer:\n",
        "import torch\n",
        "\n",
        "def softmax(x):\n",
        "  x = x - torch.max(x) # numerical stability\n",
        "  exp_x = torch.exp(x)\n",
        "  return exp_x / torch.sum(exp_x)\n",
        "\n",
        "x = torch.tensor([2.0, 1.0, 0.1])\n",
        "print(softmax(x))"
      ],
